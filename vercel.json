{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npx prisma generate && npm run build",
  "installCommand": "npm install",
  "env": {
    "DATABASE_URL": "@database_url"
  },
  "functions": {
    "api/**": {
      "runtime": "nodejs18.x"
    }
  }
}
